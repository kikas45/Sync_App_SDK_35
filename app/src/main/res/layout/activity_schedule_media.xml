<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/parentConatiner"
    tools:context=".additionalSettings.MaintenanceActivity">

    <ImageView
        android:id="@+id/backgroundImage"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/action_bar_root"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/close_bs"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:background="?selectableItemBackground"
            android:clickable="true"
            android:focusable="true"
            android:padding="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/ic_baseline_arrow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView42"
            style="@style/Theme.RemoteWebViewLight"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:gravity="center|start"
            android:maxLines="1"
            android:text="Schedule Media"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/close_bs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/close_bs"
            app:layout_constraintTop_toTopOf="@+id/close_bs" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/divider21"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_bar_root" />


    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestesScroll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        android:visibility="visible"
        tools:visibility="visible"
        android:fitsSystemWindows="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_bar_root">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/allSchedulesCard"
                style="@style/textColorStyle"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:clickable="true"
                android:elegantTextHeight="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:gravity="start|center"
                android:maxLines="1"
                android:paddingStart="65dp"
                android:paddingLeft="24dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="View All Schedule"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider49"
                tools:ignore="HardcodedText,RtlSymmetry" />


            <TextView
                android:id="@+id/addScheduleCard"
                style="@style/textColorStyle"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:clickable="true"
                android:elegantTextHeight="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:gravity="start|center"
                android:maxLines="1"
                android:paddingStart="65dp"
                android:paddingLeft="24dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="Add Schedule"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText,RtlSymmetry" />


            <TextView
                android:id="@+id/textScheduleIndicator"
                style="@style/textColorStyle"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:clickable="true"
                android:elegantTextHeight="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:gravity="start|center"
                android:maxLines="1"
                android:paddingStart="65dp"
                android:paddingLeft="24dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="Use Local Schedule"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider34"
                tools:ignore="HardcodedText,RtlSymmetry" />


            <TextView
                android:id="@+id/textGoTowebview"
                style="@style/textColorStyle"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:clickable="true"
                android:elegantTextHeight="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:gravity="start|center"
                android:maxLines="1"
                android:paddingStart="65dp"
                android:paddingLeft="24dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="Go to Web view"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider32"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="HardcodedText,RtlSymmetry" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/scheduleSwitch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:alpha="1"
                android:checked="true"
                app:layout_constraintBottom_toTopOf="@+id/divider32"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textScheduleIndicator" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/ic_arrow_move_front"
                app:layout_constraintBottom_toTopOf="@+id/divider34"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider49"
                app:tint="#494646" />


            <ImageView
                android:id="@+id/imageView47"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/ic_arrow_move_front"
                app:layout_constraintBottom_toBottomOf="@+id/textGoTowebview"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textScheduleIndicator"
                app:tint="#494646" />

            <ImageView
                android:id="@+id/imageView40"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/ic_arrow_move_front"
                app:layout_constraintBottom_toTopOf="@+id/divider49"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/addScheduleCard"
                app:tint="#494646" />


            <ImageView
                android:id="@+id/imageView41"
                style="@style/imageStyleColorTint"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:padding="2dp"
                android:layout_marginStart="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/addScheduleCard"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/addScheduleCard"
                android:src="@drawable/ic_schedule" />

            <ImageView
                android:id="@+id/imageView45"
                style="@style/imageStyleColorTint"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:padding="2dp"
                android:layout_marginStart="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/allSchedulesCard"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/allSchedulesCard"
                android:src="@drawable/ic_schedule" />


            <ImageView
                android:id="@+id/imageView46"
                style="@style/imageStyleColorTint"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:padding="2dp"
                android:layout_marginStart="20dp"
                app:layout_constraintBottom_toTopOf="@+id/divider32"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textScheduleIndicator"
                android:src="@drawable/ic_schedule" />

            <ImageView
                android:id="@+id/imageView48"
                style="@style/imageStyleColorTint"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:padding="2dp"
                android:layout_marginStart="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/textGoTowebview"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider32"
                android:src="@drawable/ic_app_or_tv_tv_24" />

            <View
                android:id="@+id/divider32"
                style="@style/dividerBackColor"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textScheduleIndicator" />

            <View
                android:id="@+id/divider50"
                style="@style/dividerBackColor"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textGoTowebview" />

            <View
                android:id="@+id/divider34"
                style="@style/dividerBackColor"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/allSchedulesCard" />

            <View
                android:id="@+id/divider49"
                style="@style/dividerBackColor"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/addScheduleCard" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>





</androidx.constraintlayout.widget.ConstraintLayout>